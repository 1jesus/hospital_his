<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhiyou.mapper.AoplogMapper">

    <insert id="insertLog">
        insert into aoplog (ip,`user`,`desc`,methodname,`date`) values
        (#{ip},#{user},#{desc},#{methodname},#{date})
    </insert>

</mapper>